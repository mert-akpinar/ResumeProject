@model  ResumeProject.Models.tblProfile
@{
    ViewBag.Title = "AddProfile";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<h2>Profil Ekleme Sayfası</h2>
<form method="post">
    @Html.HiddenFor(x => x.ProfileID)
    <br />
    <label> Profil Başlık</label>
    @Html.TextBoxFor(x => x.ProfileTitle, new { @class = "form-control" })
    <br />
    <label> Profil Açıklama</label>
    @Html.TextBoxFor(x => x.ProfileDescription, new { @class = "form-control" })
    <br />
    <label> İsim</label>
    @Html.TextBoxFor(x => x.Name, new { @class = "form-control" })
    <br />
    <label> Mail</label>
    @Html.TextBoxFor(x => x.Mail, new { @class = "form-control" })
    <br />
    <label> Adres</label>
    @Html.TextBoxFor(x => x.Adress, new { @class = "form-control" })
    <br />
    <label> Telefon</label>
    @Html.TextBoxFor(x => x.Phone, new { @class = "form-control" })
    <br />
    <label>Sosyal Media1</label>
    @Html.TextBoxFor(x => x.SocilaMedia1, new { @class = "form-control" })
    <br />
    <label>Sosyal Media2</label>
    @Html.TextBoxFor(x => x.SocialMedia2, new { @class = "form-control" })
    <br />
    <label>Sosyal Media3</label>
    @Html.TextBoxFor(x => x.SocialMedia3, new { @class = "form-control" })
    <br />
    <label>Sosyal Media4</label>
    @Html.TextBoxFor(x => x.SocialMedia4, new { @class = "form-control" })
    <br />
    <button class="btn btn-primary">Verileri Kaydet</button>

</form>
